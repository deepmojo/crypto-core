{
  "$schema": "https://typedoc.org/schema.json",
  "name": "DeepMojo Crypto-Core Documentation",
  "readme": "none",
  "entryPoints": [
    "./src/index.ts"
  ],
  "out": "docs",
  "entryPointStrategy": "expand",
  "excludeExternals": true,
  "plugin": [],
  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": false
  },
  "navigationLinks": {
    "GitHub": "https://github.com/deepmojo/crypto-core"
  },
  "categorizeByGroup": true,
  "hideGenerator": true,
  "searchInComments": true,
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": false
  },
  "sort": ["source-order"]
}
